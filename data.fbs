table Draws {
	drawBuffers: [DrawBuffer];
}

table DrawBuffer {
    buf: [ubyte] (nested_flatbuffer: 'Draw');
}

table Draw {
	id: uint16;
	name: string;
	tags: [string];
	svg: Element;
	colors: [string]; // :12 >1.11.0
}

table Element {
	name: string;
	attrs: [Attr];
	children: [Element];
}

table Attr {
	k: string;
	v: string;
}

