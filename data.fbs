table Draws {
	drawBuffers: [DrawBuffer];
}

table DrawBuffer {
	id: uint16;
    buf: [ubyte] (nested_flatbuffer: 'Draw');
}

table Draw {
	color: string;
	colors: [string];
	height: uint16;
	name: string;
	svg: Element;
	tags: [string];
	width: uint16;
}

table Element {
	name: string;
	attrs: [Attr];
	children: [Element];
}

table Attr {
	k: string;
	v: string;
}
